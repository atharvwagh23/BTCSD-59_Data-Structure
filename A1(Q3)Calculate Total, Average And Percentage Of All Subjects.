#include <stdio.h>
int main() {
    float english, math, science, history, marathi;
    float total, average, percentage;
    printf("Enter the marks for English: ");
    scanf("%f", &english);
    printf("Enter the marks for Math: ");
    scanf("%f", &math);
    printf("Enter the marks for Science: ");
    scanf("%f", &science);
    printf("Enter the marks for History: ");
    scanf("%f", &history);
    printf("Enter the marks for Marathi: ");
    scanf("%f", &marathi);
    total = english + math + science + history + marathi;
    average = total / 5.0;
    percentage = (total / 500.0) * 100;
    printf("\nThe Total marks is:  \t%.2f / 500.00\n", total);
    printf("\nThe Average marks is:\t%.2f\n", average);
    printf("\nThe Percentage is:   \t%.2f%%\n", percentage);
    return 0;
}
